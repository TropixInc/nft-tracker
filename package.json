{
  "name": "nft-tracker",
  "version": "0.0.10",
  "description": "",
  "author": "Gislainy Crisostomo <gislainycrisostomo@gmail.com>",
  "private": false,
  "license": "UNLICENSED",
  "bin": {
    "crun": "dist/main"
  },
  "scripts": {
    "check-types": "tsc --incremental false --noEmit",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npm run migrate:prod && node dist/src/main",
    "command": "node dist/src/command.js gen-operator-jwt",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "echo 'Enter the Migration file name:' && read filename && npm run typeorm migration:create \"src/modules/database/migrations/${filename}\"",
    "typeorm:migrate": "echo 'Enter the Migration file name:' && read filename && npm run typeorm migration:generate -- --dataSource src/ormconfig.ts \"src/modules/database/migrations/${filename}\"",
    "typeorm:run": "npm run typeorm migration:run -- --dataSource src/ormconfig.ts",
    "typeorm:revert": "npm run typeorm migration:revert -- --dataSource src/ormconfig.ts",
    "typeorm:show": "npm run typeorm migration:show -- --dataSource src/ormconfig.ts",
    "typeorm:log": "npm run typeorm schema:log -- --dataSource src/ormconfig.ts",
    "migrate:prod": "./node_modules/typeorm/cli.js migration:run --dataSource ./dist/src/ormconfig.js",
    "release": "standard-version",
    "git:add": "git add --all",
    "git:commit": "git-cz",
    "git": "npm run git:add && npm run git:commit",
    "doc:generate": "compodoc -p tsconfig.json",
    "doc:serve": "compodoc -p tsconfig.json -s --port 9999",
    "dc:up": "docker-compose up",
    "dc:down": "docker-compose down",
    "infra:gen": "npx zx scripts/generate-env-change.mjs"
  },
